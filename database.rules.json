{
    "rules": {
        ".read": false,
        "clubs": {
            ".read": true,
            ".write": "root.child('users').child(auth.uid).child('role').val() == 99"
        },
        "counters": {
            ".read": "root.child('users').child(auth.uid).child('role').val() == 99",
            ".write": "root.child('users').child(auth.uid).child('role').val() == 99"
        },
        "images": {
            ".read": true,
            ".write": "root.child('users').child(auth.uid).child('role').val() == 99"
        },
        "players": {
            ".read": true,
            ".write": "root.child('users').child(auth.uid).child('role').val() == 99"
        },
        "requests": {
            ".read": true,
            ".write": "root.child('users').child(auth.uid).child('role').val() > 21",
            "$request": {
                "playersList": {
                    ".read": true,
                    ".write": "root.child('users').child(auth.uid).child('role').val() > 21",
                    "$player": {
                        "playerId": {
                            ".read": true,
                            ".write": "root.child('users').child(auth.uid).child('role').val() > 21"
                        },
                        "docsChecked": {
                            ".read": true,
                            ".write": "root.child('users').child(auth.uid).child('role').val() > 51"
                        },
                        "paidChecked": {
                            ".read": true,
                            ".write": "root.child('users').child(auth.uid).child('role').val() > 51"
                        }
                    }
                },
                "manager": {
                    ".read": true,
                    ".write": "root.child('users').child(auth.uid).child('role').val() > 21"
                },
                "continue": {
                    ".read": true,
                    ".write": "root.child('users').child(auth.uid).child('role').val() > 21"
                },
                "resolved": {
                    ".read": true,
                    ".write": "root.child('users').child(auth.uid).child('role').val() > 51"
                }
            }
        },
        "transfers": {
            ".read": true,
            ".write": "root.child('users').child(auth.uid).child('role').val() == 99"
        },
        "roles": {
            ".read": true,
            ".write": "root.child('users').child(auth.uid).child('role').val() == 99"
        },
        "users": {
            "$user_id": {
                ".read": true,
                ".write": true,
                "role": {
                    ".read": true,
                    ".write": "root.child('users').child(auth.uid).child('role').val() == 99"
                }
            },
            ".read": true,
            ".write": true
        }
    }
}