{
  "name": "floorball-web",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.3.1",
    "fuse.js": "^3.3.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-cookie": "^3.0.0",
    "react-dom": "^16.4.2",
    "react-firebase-file-uploader": "^2.4.2",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-md": "^1.4.2",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.7",
    "react-router": "3.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "yarn translations && REACT_APP_FIREBASE_CONFIG=development npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "yarn translations && REACT_APP_FIREBASE_CONFIG=production npm-run-all build-css build-js",
    "build-dev": "yarn translations && REACT_APP_FIREBASE_CONFIG=development npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "translations": "node ./translationRunner.js"
  },
  "devDependencies": {
    "react-intl-translations-manager": "^5.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
