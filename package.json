{
  "name": "floorball",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "firebase": "8.6.8",
    "fuse.js": "3.3.0",
    "i18n-iso-countries": "3.7.8",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-cookie": "3.0.0",
    "react-dom": "17.0.2",
    "react-firebase-file-uploader": "2.4.2",
    "react-helmet": "5.2.0",
    "react-intl": "2.4.0",
    "react-redux": "7.2.4",
    "react-redux-firebase": "3.10.0",
    "react-router-dom": "5.2.1",
    "react-scripts": "4.0.3",
    "redux": "4.0.0",
    "sass": "^1.38.2",
    "web-vitals": "1.1.2",
    "webfontloader": "1.6.28"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm run translations && REACT_APP_FIREBASE_CONFIG=development npm-run-all -p start-js",
    "build-js": "react-scripts build",
    "build": "npm run translations && REACT_APP_FIREBASE_CONFIG=production npm-run-all build-css build-js",
    "build-dev": "npm run translations && REACT_APP_FIREBASE_CONFIG=development npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "translations": "node ./translationRunner.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
